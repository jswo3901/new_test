<%-layout('./layouts/layout')%>
<%- stylesheet('/css/main.css') %>
<%-partial('./partials/list')%>
<form action="/new" method="post">
  <div class="form-group">
    <input type="text" name="title" class="form-control" id="title" placeholder="제목">
  </div>
  <div class="form-group">
    <input type="text" name="content" class="form-control" id="content" placeholder="내용">
  </div>
  <button type="submit" class="btn btn-default" id="submit">작성</button>
</form>